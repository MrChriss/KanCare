<h1>hello to user#edit</h1>
<%= form_for @user do |f| %>
  <%= f.label :user_name %>
  <%= f.text_field :nick_name %>

  <%= f.label :email %>
  <%= f.email_field :email %>

  <%= f.label :avatar %>
  <%= f.file_field :avatar %>

  <%= f.submit "Update" %>
<% end %>
<%= link_to 'Back', new_care_action_path %>
